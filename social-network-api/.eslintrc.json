{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": "off",
    "eol-last": "off",
    "import/extensions": "off",
    "camelcase": "off",
    "quotes": ["error", "double"],
    "no-trailing-spaces": "off",
    "func-names": "off",
    "comma-dangle": "off",
    "object-curly-newline": "off",
    "no-underscore-dangle": "off",
    "prefer-destructuring": "off",
    "no-param-reassign": "off",
    "space-before-function-paren": "off",
    "max-len": "off",
    "arrow-parens": "off"
  }
}